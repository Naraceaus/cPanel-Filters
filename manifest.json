{
  "manifest_version": 2,

  "name": "cPanel Helper",
  "description": "A series of small functions that make using the Neto cPanel simpler",
  "version": "4.0",
		"options_ui":{
			"page":"options.html",
			"chrome_style": true
		},
  "browser_action": {
    "default_icon": "icon.png"
  },
		"background":{
				"persistent": false,
				"scripts": ["start_up.js"]
		},
		"content_scripts": [
			{
					"matches": ["<all_urls>"],
					"js": ["content_creation.js", "page_analysis.js","pageload.js"],
					"run_at": "document_end"
			}
  ],
  "permissions": [
    "activeTab",
	"tabs",
	"storage"
  ]
}