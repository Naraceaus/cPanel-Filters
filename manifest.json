{
  "manifest_version": 2,

  "name": "cPanel Helper",
  "description": "A series of small functions that make using the Neto cPanel simpler",
  "version": "5.2",
	"options_ui":{
		"page":"options.html",
		"chrome_style": true
	},
	"browser_action": {
	"default_icon": "icon.png"
	},
	"background":{
			"persistent": false,
			"scripts": ["start_up.js"]
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content_creation.js", "page_analysis.js","pageload.js"],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": [
		"cross.svg",
		"drag.png",
		"arrow_full_up.svg",
		"arrow_full_right.svg",
		"arrow_full_down.svg",
		"arrow_full_left.svg"
	],
	"permissions": [
		"activeTab",
		"tabs",
		"storage",
		"cookies",
		"*://*/*"
	]
}