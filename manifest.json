{
  "manifest_version": 2,

  "name": "cPanel Filters",
  "description": "Generate cPanel URLS that match the filter on the page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
		"content_scripts": [
			{
					"matches": ["<all_urls>"],
					"js": ["pageload.js"],
					"run_at": "document_end"
			}
  ],
  "permissions": [
    "tabs"
  ]
}
